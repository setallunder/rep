<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="field" onKeyPressed="#KeyboardHandle" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.SnakesWindowController">
   <children>
      <MenuBar layoutX="80.0" layoutY="60.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="Game">
            <items>
                  <MenuItem fx:id="start" mnemonicParsing="false" onAction="#StartHandle" text="Start" />
                  <MenuItem fx:id="showLastGame" mnemonicParsing="false" onAction="#ShowLastGameHandle" text="Show last game" />
                  <MenuItem fx:id="autoPlay" mnemonicParsing="false" onAction="#AutoPlayHandle" text="Auto play" />
              <MenuItem fx:id="end" mnemonicParsing="false" onAction="#EndHandle" text="End" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Circle fx:id="apple" fill="#e8141f" layoutX="118.0" layoutY="92.0" radius="5.0" stroke="BLACK" strokeType="INSIDE" />
      <Rectangle fx:id="head" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="10.0" layoutX="295.0" layoutY="195.0" stroke="BLACK" strokeType="INSIDE" width="10.0" />
   </children>
</AnchorPane>
